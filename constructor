class Person:
#    name = "rohan"
#    occ = "Python developer"
    def __init__(self,n,o):
        print("hey iam a person")
        self.name = n
        self.occ = o

    def info(self):
        print(f"{self.name} is a {self.occ}")


a = Person("rohan" , "developer")
b = Person("vedant" , "developer")
a.info()
b.info()
